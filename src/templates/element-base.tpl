<div id="{name}" class="frmb-group form-group">
  <h4>{label}</h4>
  <label for="{name}_field_label">Field Label:</label>
  <input type="text" value="" placeholder="ex: First Name" name="{name}_label" id="{name}_label">
  <label for="{name}_field_required">Required?</label>
  <input type="checkbox" value="" name="{name}_required" id="{name}_required">
  <a href="#" class="frmb-remove">Remove</a>
</div>